<launch>
  <group ns="stagesim">
    <node pkg="stage_ros" name="stageros" type="stageros" args="$(find assignment3)/world/empty.world">
        <param name="use_sim_time" type="boolean" value="true" />
    </node>
    <node pkg="tf" type="static_transform_publisher" name="localizer" args="0 0 0 0 0 0 map odom 100">
        <param name="use_sim_time" type="boolean" value="true" />
    </node>
    <node pkg="assignment3" name="follow_carrot" type="follow_carrot_node" output="screen">
        <param name="use_sim_time" type="boolean" value="true" />
    </node>
    <node pkg="assignment3" name="rectangle_planner" type="rectangle_planner_node" output="screen">
        <param name="use_sim_time" type="boolean" value="true" />
    </node>
  </group>
</launch>
